{
  "name": "ProjectCatwalk",
  "version": "1.0.0",
  "description": "Front End Capstone",
  "main": "webpack.config.js",
  "scripts": {
    "react-dev": "webpack -d eval-source-map --config webpack.dev.js --watch",
    "react-prod": "webpack --config webpack.prod.js",
    "startAPI": "nodemon api_server/index.js",
    "startFast": "nodemon fast_server/index.js",
    "start": "nodemon server/index.js",
    "easystart": "concurrently --kill-others \"npm run react-dev\" \"npm run start\"",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JessicaPrestonVinnie/ProjectCatwalk.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JessicaPrestonVinnie/ProjectCatwalk/issues"
  },
  "homepage": "https://github.com/JessicaPrestonVinnie/ProjectCatwalk#readme",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
