    "nodemon": "^2.0.12",
    "react-test-renderer": "^17.0.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@newrelic/fastify": "github:newrelic-experimental/newrelic-javascript-fastify",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fastify": "^3.20.2",
    "fastify-cli": "^2.13.0",
    "fastify-formbody": "^5.1.0",
    "fastify-static": "^4.2.3",
    "jquery": "^3.6.0",
